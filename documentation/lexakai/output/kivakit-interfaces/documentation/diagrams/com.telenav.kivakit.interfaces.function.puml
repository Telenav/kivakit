
@startuml

    !include lexakai.theme
    
    title "com.telenav.kivakit.interfaces.function"
    
    interface IntMapper<Value>
    {
        --(collection)--
        +Value map(int parameter)
    }
    
    interface LongMapper<Value>
    {
        --(collection)--
        +Value map(long parameter)
    }
    
    interface Mapper<Parameter, Value>
    {
        --(collection)--
        +Value map(Parameter)
    }
    
    interface Presence
    {
        --(checks)--
        +boolean isAbsent()
        +boolean isPresent()
    }
    
@enduml

